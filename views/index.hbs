                                     {{/if}}
                                    <a href="/productShow/{{this._id}}">
                                        <!-- Image -->
                                        <img class="primary blur-up lazyload"
                                            data-src="/product-images/{{this._id}}image1.jpg"
                                            src="/product-images/{{this._id}}image1.jpg" alt="image" title="product" />
                                        <!-- End Image -->
                                        <!-- Hover Image -->
                                        <img class="hover blur-up lazyload"
                                            data-src="/product-images/{{this._id}}image2.jpg"
                                            src="/product-images/{{this._id}}image2.jpg" alt="image" title="product" />
                                        <!-- End Hover Image -->
                                        <!-- Product Label -->

                                        <!-- End Product Label -->
                                    </a>
                                    <!-- End Product Image -->

                                    <!-- Product Button -->
                                    <div class="button-set">
                                        <div class="quickview-btn" data-bs-toggle="tooltip" data-bs-placement="top"
                                            title="quick view">
                                            <a href="#open-quickview-popup{{this._id}}"
                                                class="btn quick-view-popup quick-view"><i
                                                    class="icon an an-search"></i></a>
                                        </div>
                                        <div class="variants add" data-bs-toggle="tooltip" data-bs-placement="top"
                                            title="add to cart">

                                            <a href="/cart/cart-add/{{this._id}}"
                                                class="btn cartIcon btn-addto-cart "><i
                                                    class="icon an an-shopping-bag"></i></a>

                                        </div>
                                        <div class="wishlist-btn" data-bs-toggle="tooltip" data-bs-placement="top"
                                            title="add to wishlist">
                                            <a onclick="wishlist('{{this._id}}')" class="btn open-wishlist-popup wishlist add-to-wishlist"><i
                                                    class="icon an an-heart"></i></a>
                                        </div>

                                    </div>
                                    <!-- End product button -->
                                </div>
                                <!-- End Product Image -->

                                <!-- Product Details -->
                                <div class="product-details text-center">
                                    <!-- Product Name -->
                                    <div class="product-name">
                                        <a href="/productShow/{{this._id}}">{{this.productname}}</a>
                                    </div>
                                    <!-- End Product Name -->
                                    <!-- Product Price -->
                                    <div class="product-price">
                                        <span class="price">₹{{this.productprice}}</span>
                                    </div>
                                    <!-- End Product Price -->
                                    <!-- Product Review -->
                                 
                                    <!-- End Product Review -->
                                </div>
                                <!-- End Product Details -->
                            </div>








                            {{/each}}
                        </div>
                    </div>
                    <!-- End Grid Product -->
                </div>
            </div>

            <!-- End Parallax Section -->



            <!-- Scoll Top -->
            <div id="site-scroll"><i class="icon an an-angle-up"></i></div>
            <!-- End Scoll Top -->

            <!-- Minicart Drawer -->

            <!-- End Minicart Drawer -->



            <!-- Wishlist Added Popup -->

            <!-- End Wishlist Added Popup -->


            <!-- Including Javascript -->


        </div>
        <!-- End Page Wrapper -->
    </div>
</div>

    <script>


        function wishlist(proId) {

            $.ajax({
                url: '/wishlist',
                data: {
                    proId: proId
                },
                method: 'post',
                success: (response) => {
                    if (response.status) {
                        swal("Item added to wishlist")
                    } else {
                        swal("Item already added")
                    }
                },
                error: ()  =>{
                    swal("Please LogIn")
                }
            })
        }

    </script>