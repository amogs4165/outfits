<script>
    function remove(proId) {
        swal({
            title: "Are you sure?",
            text: "Once deleted, you will not be able to recover this imaginary file!",
            icon: "warning",
            buttons: true,
            dangerMode: true,
        })
            .then((willDelete) => {
                if (willDelete) {
                    $.ajax({
                        url: "/wishlist/remove",
                        type: "POST",
                        data: {
                            
                            product: proId
                        },

                        success: (response) => {
                            if (response.status) {
                                $('#' + proId).remove()
                                swal("Poof! Your imaginary file has been deleted!", {
                                    icon: "success",
                                });


                            }


                        },

                    });

                } else {
                    swal("Your imaginary file is safe!");
                }
            });
    }
</script>