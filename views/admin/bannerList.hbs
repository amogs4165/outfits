<div id="layout-wrapper">  
            

            <!-- ========== Left Sidebar Start ========== -->

            
            <!-- Start right Content here -->
            <!-- ============================================================== -->
                <div class="main-content">

                    <div class="page-content">
                        {{!-- <div class="container-fluid"> --}}

                            <!-- start page title -->
                            <div class="row">

                                
        <table class="table" id="adminTable" >
            <thead>
                <tr >
                <th scope="col">No</th>
                 <th scope="col">Name</th>
                <th scope="col">Banner Image</th>
                <th scope="col">Top</th>
                <th scope="col">Middle</th>
                <th scope="col">Bottom</th>
                <th scope="col">Action</th>
                <th scope="col">Remove</th>
                
                </tr>
            </thead>
                <tbody >
                    {{#each banner}}
                        <tr class="align-middle">
                            <td></td>
                            <td>{{this.info.bannername}}</td>
                            <td><img src="/banner-images/{{this._id}}image1.jpg" style="width: 15rem; height: auto;" ></td>
                            <td>{{this.info.topContent}}</td>
                            <td>{{this.info.middleContent}}</td>
                            <td>{{this.info.bottomContent}}</td>
                            <td>
                                <a class="btn btn-primary mt-2 "  href="/banner/select/{{this._id}}" onclick="return confirm('click Ok to confirm')" type="submit">Select</a>
                             
                            </td>
                            <td><a class="btn btn-danger mt-2 "  href="/banner/delete/{{this._id}}" onclick="return confirm('click Ok to confirm Delete')" type="submit">Delete</a></td>
                            
                        </tr>
                    {{/each}}
                    
                </tbody>
        </table>

                            </div>
                        </div>
                    </div>
                </div>
</div>





